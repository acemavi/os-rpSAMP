{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "SAMP Pawn Compiler Libray-normal",
      "type": "shell",
      "command": "${workspaceRoot}\\pawno\\pawncc.exe",
      "args": [
        "${file}",
        "-D${fileDirname}",
        "--%",
        "-;+",
        "-(+",
        "-O1"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "isBackground": false,
    }
  ]
}